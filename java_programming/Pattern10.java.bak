class Pattern10
{

	public static void main(String[] args) 
	{
		for (int i=1;i<=5;i++)
		{
			for (int j=1;j<=5;j++)
			{
				if (i==1)
				{
				System.out.print(i);
				}
				else if (i==2 )
                {
			     System.out.print("2");
				}
				else
				{
					System.out.print(" ");
				}
			}
		
			System.out.println(); 
		}  
		
	}
				
}