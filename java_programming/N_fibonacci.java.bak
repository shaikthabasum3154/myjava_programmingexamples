import java.util.Scanner;
class  N_fibonacci
{
	public static void main(String[] args) 
	{
		Scanner scan=new Scanner(System.in);
		System.out.println("enter a value");
		int num=scan.nextInt();
		int a=0;
		int b=1;
		int sum=0;
		 int y=1;
		for (a=1;a<=num;a++)
		{
			System.out.println(y+") "+sum);
			 a=b;
			 b=sum;
			 sum=a+b;
			 y++;
			
		}

	}
}
